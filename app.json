{
  "expo": {
    "name": "AyurSathi",
    "slug": "AyurSathi",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "ayursathiapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.xeylous.AyurSathi"
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "androidNavigationBar": {
      "visible": true,
      "translucent": false,
      "backgroundColor": "#000000",
      "barStyle": "light-content"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ECF39E",
          "dark" : {
            "image": "./src/assets/images/icon.png",
            "backgroundColor": "#ECF39E"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
